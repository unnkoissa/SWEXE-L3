<!DOCTYPE html>
<html>
  <head>
    <title>Tw</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="header">
        <%= render "header" %>
    </div>
    
    
    <div id="container">
      <div id="content">
        <%= yield %>
      </div>
      
      <div class="sidebar">
        <%= render "sidebar" %>
      </div>
      
    </div>
      
      
      
      <div id="footer">
        <%= render "footer" %>
      </div>
  </body>
</html>
